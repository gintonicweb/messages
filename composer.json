{
    "name": "gintonicweb/messages",
    "description": "Messages plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Messages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Messages\\Test\\": "tests"
        }
    }
}
